{
	"package": {
		"productName": "p2pthing",
		"version": "0.1.0"
	},
	"build": {
		"distDir": "../dist",
		"devPath": "http://localhost:3000",
		"beforeDevCommand": "",
		"beforeBuildCommand": "",
		"withGlobalTauri": true
	},
	"tauri": {
		"bundle": {
			"active": true,
			"targets": "all",
			"identifier": "p2pthing",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"resources": [],
			"externalBin": [],
			"copyright": "",
			"category": "DeveloperTool",
			"shortDescription": "",
			"longDescription": "",
			"deb": {
				"depends": []
			},
			"macOS": {
				"frameworks": [],
				"minimumSystemVersion": "",
				"exceptionDomain": "",
				"signingIdentity": null,
				"entitlements": null
			},
			"windows": {
				"certificateThumbprint": null,
				"digestAlgorithm": "sha256",
				"timestampUrl": ""
			}
		},
		"updater": {
			"active": false
		},
		"allowlist": {
			"fs": {
				"readFile": true,
				"scope": ["$APPDATA/**"]
			},
			"path": {
				"all": true
			},
			"protocol": {
				"asset": true,
				"assetScope": ["$APPDATA/**"]
			}
		},
		"windows": [
			{
				"title": "p2pthing",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": "default-src blob: asset: https://asset.localhost data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
		}
	}
}
